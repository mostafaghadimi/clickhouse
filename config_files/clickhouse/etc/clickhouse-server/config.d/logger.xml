<clickhouse>
  <logger>
    <level>trace</level>
    <log>/var/log/clickhouse-server/clickhouse-server-%z-%A-%F.log</log>
    <errorlog>/var/log/clickhouse-server/clickhouse-server-%z-%A-%F.err.log</errorlog>
    <size>1000M</size>
    <count>10</count>
    <stream_compress>false</stream_compress>
    <!-- Uncomment the following line for debugging purpose and also check the logger level -->
    <console>1</console>
    <!-- end comment -->
  </logger>
</clickhouse>
